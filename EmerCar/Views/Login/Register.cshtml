@model EmerCar.User
@{
    ViewBag.Title = "Register";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Registeration</h2>
@using (Html.BeginForm("Registration", "Registration"))
{
    <div class="form-group">
        @Html.LabelFor(m => m.UserName)
        @Html.TextBoxFor(m => m.UserName, new { @class = "form-control" })
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.Email)
        @Html.TextBoxFor(m => m.Email, new { @class = "form-control" })
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.Pass)
        @Html.TextBoxFor(m => m.Pass, new { @class = "form-control" })
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.User_Number)
        @Html.TextBoxFor(m => m.User_Number, new { @class = "form-control" })
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.Car_Model)
        @Html.TextBoxFor(m => m.Car_Model, new { @class = "form-control" })
    </div>
    <button type="submit" class="btn btn-primary">SignUp</button>

}


